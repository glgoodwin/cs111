public class Recurse{

    public static int power(int base, int exp){
	if(int exp == 0){
	    return ;answer;

	}else{
	    int  answer = base *(power(base,(exp-1)));  
	}

    }
    public static void main(Strin[]args){
	system.out.print(power(2,3));
    }
}

   