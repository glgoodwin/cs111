<div id="HttpMgr" c:purpose="HttpMgr" c:async="F">
<s:event b:on="construct">
<s:variable b:name="tStatus" b:scope="tag" b:select=""/>
<s:variable b:name="tStatusText" b:scope="tag" b:select=""/>
<s:variable b:name="tResponseText" b:scope="tag" b:select=""/>
<s:variable b:name="tResponseHeaders" b:scope="tag" b:select=""/>
<s:variable b:name="tHttpObject" b:scope="tag" b:select=""/>
<s:variable b:name="tAsyncKey" b:scope="tag" b:select=""/>
</s:event>
<s:event b:on="SetHandler">
<s:variable b:name="lHandler" b:scope="local" b:select="string($pHandler)"/>
<s:variable b:name="lNode" b:scope="local" b:select="$pNode"/>
<s:script><![CDATA[

var sHandler = _vars['lHandler'][0];
var oElm = _vars['lNode'][0];
gHTTPMgr.set_handler(sHandler,oElm);

]]></s:script>
</s:event>
<s:event b:on="SetObserver">
<s:variable b:name="lCode" b:scope="local" b:select="string('*')"/>
<s:if b:test="declared($pCode)">
<s:task b:action="assign" b:target="$lCode" b:scope="local" b:select="string($pCode)"/>
</s:if>
<s:variable b:name="lHandler" b:scope="local" b:select="string($pHandler)"/>
<s:variable b:name="lNode" b:scope="local" b:select="$pNode"/>
<s:script><![CDATA[

var iCode = _vars['lCode'][0];
var sHandler = _vars['lHandler'][0];
var oElm = _vars['lNode'][0];
gHTTPMgr.set_observer(iCode,sHandler,oElm);

]]></s:script>
</s:event>
<s:event b:on="Load">
<s:variable b:name="url" b:scope="local" b:select="$pUrl"/>
<s:variable b:name="method" b:scope="local" b:select="string('GET')"/>
<s:if b:test="declared($pMethod)">
<s:task b:action="assign" b:target="method" b:scope="local" b:select="$pMethod"/>
</s:if>
<s:variable b:name="data" b:scope="local" b:select="string('')"/>
<s:if b:test="declared($pData)">
<s:task b:action="assign" b:target="data" b:scope="local" b:select="$pData"/>
</s:if>
<s:variable b:name="destination" b:scope="local" b:select="$tResponseText"/>
<s:if b:test="declared($pDestination)">
<s:task b:action="assign" b:target="destination" b:scope="local" b:select="$pDestination"/>
</s:if>
<s:variable b:name="mode" b:scope="local" b:select="string('replacechildren')"/>
<s:if b:test="declared($pMode)">
<s:task b:action="assign" b:target="data" b:scope="local" b:select="$pMode"/>
</s:if>
<s:choose>
<s:when b:test="$method eq 'GET'">
<s:task b:action="load" b:url="{$url}" b:method="{$method}" b:data="{$data}" b:destination="$destination" b:mode="{$mode}"/>
</s:when>
<s:otherwise>
<s:choose>
<s:when b:test="contains($data,'?&gt;')">
<s:task b:action="load" b:url="{$url}" b:method="{$method}" b:data="{$data}" b:destination="$destination" b:mode="{$mode}">
<s:httpheader b:name="Content-Type" b:value="text/xml"/>
</s:task>
</s:when>
<s:otherwise>
<s:task b:action="load" b:url="{$url}" b:method="{$method}" b:data="{$data}" b:destination="$destination" b:mode="{$mode}"/>
</s:otherwise>
</s:choose>
</s:otherwise>
</s:choose>
</s:event>
<s:event b:on="Reset">
<s:script><![CDATA[

gHTTPMgr.reset();

]]></s:script>
</s:event>
<s:event b:on="HandleHttpResponse">
<s:variable b:name="lCode" b:scope="local" b:select="string($tStatus)"/>
<s:script><![CDATA[

var iCode = _vars['lCode'][0];
var oHndl = {};
var aHndl = [];
if ( (oHndl = gHTTPMgr.get_handler()) ) {
bpc.trigger(oHndl.sHandler,oHndl.oElm);
}
if ( (aHndl = gHTTPMgr.get_observer(iCode)) ) {
for ( var i=0; i < aHndl.length; i++ ) {
bpc.trigger(aHndl[i].sHandler,aHndl[i].oElm);
}
}

]]></s:script>
</s:event>
</div>
</div>