<s:when b:test="$lRsrvWizEditGroup/@b:state = 'selected' and count($lRsrvWizEditBuffers) gt 0">
<s:choose>
<s:when b:test="$lRsrvWizEditBuffers/@b:state = 'selected'">
<s:task b:action="trigger" b:event="Select" b:target="$lRsrvWizEditBuffers[@b:state = 'selected']"/>
</s:when>
<s:otherwise>
<s:task b:action="trigger" b:event="Select" b:target="$lRsrvWizEditBuffers[1]"/>
</s:otherwise>
</s:choose>
</s:when>
<s:when b:test="$lRsrvWizCreateBuffers/@b:state = 'selected'">
<s:task b:action="trigger" b:event="Select" b:target="$lRsrvWizCreateBuffers[@b:state='selected']"/>
</s:when>
<s:when b:test="$lRsrvWizEditBuffers/@b:state = 'selected'">
<s:task b:action="trigger" b:event="Select" b:target="$lRsrvWizEditBuffers[@b:state='selected']"/>
</s:when>
<s:otherwise>
<s:task b:action="trigger" b:event="Select" b:target=".//div[@b:behavior='LayoutTabboxBuffer'][last()]"/>
</s:otherwise>
</s:choose>
</s:with>
</s:if>
<s:task b:action="select" b:target="id('layout-subtabbar')/div[@c:purpose = $tGroupUmbrella]//s25:tabitem[contains($pPurpose, @c:purpose)]"/>
<s:if b:test="$pPurpose = 'publisher'">
<s:task b:action="trigger" b:event="Select" b:target=".//s25:publisher_admin"/>
</s:if>
</s:if>
</s:event>
</s:behavior>
<s:behavior b:name="LayoutTabboxBuffer">
<s:initatt class="layout-tabbox-buffer" c:dirty="true"/>
<s:event b:on="construct">
<s:super/>
<s:variable b:name="tGroupUmbrella" b:scope="tag" b:select="string(ancestor-or-self::div[@b:behavior='LayoutTabboxGroup' or @b:behavior='LayoutTabboxBuffer'][last()]/@c:purpose)"/>
<s:setatt c:init="1"/>
</s:event>
<s:event b:on="Select">
<s:task b:action="trigger" b:event="DismissAllTips" b:target="."/>
<s:choose>
<s:when b:test="@b:state = 'selected'"> 
<s:task b:action="trigger" b:event="select" b:target="."/>
</s:when>
<s:otherwise>
<s:task b:action="select" b:target="."/>
</s:otherwise>
</s:choose>
</s:event>
<s:event b:on="select">
<s:if b:test="@c:init = '1'">
<s:variable b:name="pPurpose" b:select="string(@c:purpose)"/>
<s:if b:test="@c:dirty = 'true'">
<s:setatt c:dirty="false"/>
<s:task b:action="trigger" b:event="PaintBufferContent"/>
</s:if>
<s:task b:action="select" b:target="ancestor::div[@b:behavior='LayoutTabboxGroup' or @b:behavior='LayoutTabboxBuffer']"/>
<s:task b:action="select" b:target="id('layout-tabbar')/s25:tabitem[@c:group = $tGroupUmbrella]"/>
<s:task b:action="select" b:target="id('layout-subtabbar')/div[@c:purpose = $tGroupUmbrella]"/>
<s:task b:action="select" b:target="id('layout-subtabbar')/div[@c:purpose = $tGroupUmbrella]//s25:tabitem[contains($pPurpose, @c:purpose)]"/>
<s:if b:test="string(id('FLS_SPACE_SEARCH')) = 'F'">
<s:task b:action="show" b:target="id('layout-subtabbar')//s25:tabitem[@c:purpose='space_search']"/>
<s:task b:action="show" b:target="id('layout-subtabbar')//s25:tabitem[@c:purpose='space_browse']"/>        
</s:if>
<s:if b:test="string(id('FLS_EVENT_SEARCH')) = 'F'">
<s:task b:action="show" b:target="id('layout-subtabbar')//s25:tabitem[@c:purpose='event_search']"/>
<s:task b:action="show" b:target="id('layout-subtabbar')//s25:tabitem[@c:purpose='event_browse']"/>        
</s:if>
<s:if b:test="string(id('FLS_RESOURCE_SEARCH')) = 'F'">
<s:task b:action="show" b:target="id('layout-subtabbar')//s25:tabitem[@c:purpose='resource_search']"/>
<s:task b:action="show" b:target="id('layout-subtabbar')//s25:tabitem[@c:purpose='resource_browse']"/>        
</s:if>
<s:task b:action="trigger" b:event="SaveBookmark" b:target="id('LayoutMgr')"/>
</s:if>
</s:event>
<s:event b:on="PaintBufferContent">
<s:task b:action="trigger" b:event="DismissAllTips" b:target="."/>
<s:choose>
<s:when b:test="@c:purpose = 'home_my25live'">
<s:task b:action="trigger" b:event="PaintMy25Live"/>
</s:when>
<s:when b:test="@c:purpose = 'home_calendar'">
<s:task b:action="trigger" b:event="PaintHomeCalendar"/>
</s:when>
<s:when b:test="$pPurpose = 'event_search'">
<s:task b:action="trigger" b:event="PaintEventSearch"/>
</s:when>
<s:when b:test="$pPurpose = 'event_browse'">
<s:task b:action="trigger" b:event="PaintEventBrowse"/>
</s:when>
<s:when b:test="$pPurpose = 'event_advanced'">
<s:task b:action="trigger" b:event="PaintEventSearchAdvanced"/>
</s:when>
<s:when b:test="starts-with($pPurpose, 'event_details')">
<s:task b:action="trigger" b:event="PaintEventDetails"/>
</s:when>
<s:when b:test="$pPurpose = 'space_search'">
<s:task b:action="trigger" b:event="PaintSpaceSearch"/>
</s:when>
<s:when b:test="$pPurpose = 'space_browse'">
<s:task b:action="trigger" b:event="PaintSpaceBrowse"/>
</s:when>
<s:when b:test="starts-with($pPurpose, 'space_details')">
<s:task b:action="trigger" b:event="PaintSpaceDetails"/>
</s:when>
<s:when b:test="$pPurpose = 'resource_search'">
<s:task b:action="trigger" b:event="PaintResourceSearch"/>
</s:when>
<s:when b:test="$pPurpose = 'resource_browse'">
<s:task b:action="trigger" b:event="PaintResourceBrowse"/>
</s:when>
<s:when b:test="starts-with($pPurpose, 'resource_details')">
<s:task b:action="trigger" b:event="PaintResourceDetails"/>
</s:when>
<s:when b:test="$pPurpose = 'task_overview'">
<s:task b:action="trigger" b:event="PaintTaskOverview"/>
</s:when>
<s:when b:test="$pPurpose = 'task_agenda'">
<s:task b:action="trigger" b:event="PaintTaskAgenda"/>
</s:when>
<s:when b:test="$pPurpose = 'event_reports'">
<s:task b:action="trigger" b:event="PaintEventReports"/>
</s:when>
<s:when b:test="$pPurpose = 'space_reports'">
<s:task b:action="trigger" b:event="PaintSpaceReports"/>
</s:when>		
<s:when b:test="$pPurpose = 'resource_reports'">
<s:task b:action="trigger" b:event="PaintResourceReports"/>
</s:when>
<s:when b:test="$pPurpose = 'other_reports'">
<s:task b:action="trigger" b:event="PaintOtherReports"/>
</s:when>		
<s:when b:test="$pPurpose = 'publisher_overview'">
<s:task b:action="trigger" b:event="PaintPublisherOverview"/>
</s:when>
<s:when b:test="starts-with($pPurpose, 'rsrvwiz')">
<s:task b:action="trigger" b:event="PaintRsrvWizard"/>
</s:when>
</s:choose>
<s:task b:target="id('bxLoading')" b:action="trigger" b:event="HideLoading"/>
</s:event>
