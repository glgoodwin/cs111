<s:event b:on="SaveBookmark">
<s:sequential><s:lock>
<s:task b:action="assign" b:target="$tBookmark" b:scope="tag" b:select="string(id('LayoutMgr')/$tTabbox//div[@b:behavior='LayoutTabboxBuffer'][@b:state='selected'][not(ancestor::div[@b:behavior='LayoutTabboxGroup'][not(@b:state='selected')])]/@c:purpose)"/>
<s:if b:test="contains($tBookmark,'_details')">
<s:variable b:name="lType" b:select="substring-before($tBookmark, '_details_')"/>
<s:variable b:name="lId" b:select="substring-after($tBookmark, '_details_')"/>
<s:variable b:name="lTaskType" b:select="''"/>
<s:if b:test="contains($lId, '_')">
<s:task b:action="assign" b:target="$lTaskType" b:select="substring-before($lId, '_')"/>
<s:task b:action="assign" b:target="$lId" b:select="substring-after($lId, '_')"/>
</s:if>
<s:task b:action="assign" b:target="$tBookmark" b:scope="tag" b:select="concat('details', '&amp;obj_type=',$lType, '&amp;obj_id=',$lId)"/>
<s:task b:test="not($lTaskType = '')" b:action="assign" b:target="$tBookmark" b:scope="tag" b:select="concat($tBookmark, '&amp;task_type=',$lTaskType)"/>
</s:if>
<s:variable b:name="lCurrentBookmark" b:scope="local" b:select="''"/>
<s:script><![CDATA[

var s = String(_vars['tBookmark'][0]);
s = s.replace(/%/g,'{percent}');
bpc.setVariable('tBookmark',[encodeURI(s)],'tag');
var currentBookmark = location.hash.substring(1);
currentBookmark = currentBookmark.replace(/\[(.*)\]/, '');
bpc.setVariable('lCurrentBookmark', [currentBookmark], 'local'); 

]]></s:script>
<s:if b:test="not($lCurrentBookmark = $tBookmark) or $tFirstBookmark = 'T'">
<s:history b:name="browser" b:bookmark="{$tBookmark}">
<s:task b:target="id('Controller')" b:action="trigger" b:event="BookmarkCheck"/>
</s:history>
<s:task b:test="$tFirstBookmark = 'T'" b:action="assign" b:scope="tag" b:target="$tFirstBookmark" b:select="'F'"/>
</s:if>
</s:lock></s:sequential>
<s:variable b:name="lPubFLS" b:scope="local" b:select="string(id('FLS_VCAL_PUB'))"/>
<s:variable b:name="lLoggedIn" b:scope="local" b:select="string(id('WSSessionMgr')/@c:logged-in)"/>
<s:choose>
<s:when b:test="not($lLoggedIn = 'false') and $lPubFLS = 'F'">
<s:task b:action="show" b:target="$tTabbar/s25:tabitem[@c:group='publisher']"/>
</s:when>
<s:otherwise>
<s:task b:action="hide" b:target="$tTabbar/s25:tabitem[@c:group='publisher']"/>
</s:otherwise>
</s:choose>
<s:variable b:name="lTaskFLS" b:scope="local" b:select="string(id('FLS_TASK_LIST'))"/>
<s:choose>
<s:when b:test="not($lLoggedIn = 'false') and $lTaskFLS = 'F'">
<s:task b:action="show" b:target="$tTabbar/s25:tabitem[@c:group='task']"/>
</s:when>
<s:otherwise>
<s:task b:action="hide" b:target="$tTabbar/s25:tabitem[@c:group='task']"/>
</s:otherwise>
</s:choose>
</s:event>
<s:event b:on="ViewReleaseNotes">
<s:script><![CDATA[

var historyURL = 'app/history.html';
var pathname = location.pathname;
if (instance_type == 'hybrid') {
historyURL = s25live_version + '/' + 'app/history.html';
pathname = location.pathname + 'local/';
}
window.open(historyURL + '?pathname=' + pathname, 'history');

]]></s:script>
</s:event>
<s:event b:on="ViewObjectDetail">
<s:variable b:name="lObj" b:select="id('Controller')/$tInbox/selected_obj"/>
<s:variable b:name="lPurpose" b:select="concat($lObj/@obj_type, '_details_', $lObj/@obj_id)"/>
<s:choose>
<s:when b:test="not($tTabbox//div[@b:behavior='LayoutTabboxBuffer' and @c:purpose = $lPurpose])">
<s:with b:target="id('LayoutMgr')/$tTabbar/s25:tabitem[@c:group = $lObj/@obj_type]">
<s:setatt c:start_dt="{$lObj/@start_dt}" c:end_dt="{$lObj/@end_dt}"/>
</s:with>
<s:render b:destination="$tTabbox//div[@b:behavior='LayoutTabboxGroup'][@c:purpose = concat($lObj/@obj_type, '_details')]/b:deck" b:mode="aslastchild">
<div b:behavior="LayoutTabboxBuffer" c:purpose="{$lPurpose}" c:obj_type="{$lObj/@obj_type}" c:obj_id="{$lObj/@obj_id}" c:date="{$lObj/@date}">
<div> </div>
</div>
</s:render>
<s:task b:action="trigger" b:event="Select" b:target="$tTabbox//b:deck/div[@c:purpose = $lPurpose]"/>
</s:when>
<s:otherwise>
<s:variable b:name="lDetailFlush" b:select="'F'"/>
<s:choose>
<s:when b:test="$lObj/@obj_type = 'event' and not(string-length($lObj/@date)) = 0">
<s:variable b:name="lEventDetailBuffer" b:select="id('layout-tabbox-groups')/div[@c:purpose='event']//div[@c:purpose = $lPurpose]"/>
<s:variable b:name="lCurrentInstanceDate" b:select="$lEventDetailBuffer/@c:date"/>
<s:variable b:name="lNewInstanceDate" b:select="$lObj/@date"/>
<s:if b:test="not($lNewInstanceDate = $lCurrentInstanceDate)">
<s:task b:action="assign" b:target="$lDetailFlush" b:select="'T'"/>
<s:with b:target="$lEventDetailBuffer">
<s:setatt c:date="{$lNewInstanceDate}" c:dirty="true"/>
</s:with>
</s:if>
</s:when>
<s:when b:test="($lObj/@obj_type = 'space' or $lObj/@obj_type = 'resource') and string-length($lObj/@start_dt) gt 0 and $lDetailFlush = 'F'">
<s:variable b:name="lDetailControl" b:select="id('layout-tabbox-groups')/div[@c:purpose = $lObj/@obj_type]//div[@c:purpose = concat($lObj/@obj_type, '_details_', $lObj/@obj_id)]/*"/>
<s:variable b:name="lCurrentStartDate" b:select="$lDetailControl/@c:start_dt"/>
<s:variable b:name="lNewStartDate" b:select="$lObj/@start_dt"/>
<s:variable b:name="lCurrentEndDate" b:select="$lDetailControl/@c:end_dt"/>
<s:variable b:name="lNewEndDate" b:select="$lObj/@end_dt"/>
<s:if b:test="not($lNewStartDate = $lCurrentStartDate) or not($lNewEndDate = $lCurrentEndDate)">
<s:task b:action="assign" b:target="$lDetailFlush" b:select="'T'"/>
<s:with b:target="id('LayoutMgr')/$tTabbar/s25:tabitem[@c:group = $lObj/@obj_type]">
<s:setatt c:start_dt="{$lObj/@start_dt}" c:end_dt="{$lObj/@end_dt}"/>
</s:with>
<s:with b:target="$lDetailControl/ancestor::div[@b:behavior='LayoutTabboxBuffer']">
<s:setatt c:dirty="true"/>
</s:with>
</s:if>
</s:when>
</s:choose>
