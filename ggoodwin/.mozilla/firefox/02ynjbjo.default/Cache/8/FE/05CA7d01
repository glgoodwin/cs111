<s:task b:action="trigger" b:event="ShowLoading" b:target="."/>
</s:with>
<s:with b:target="id('layout-subtabbar-tasks')">
<s:render b:test="not(./s25:subtabbar)" b:destination="." b:mode="replacechildren">
<s25:subtabbar c:purpose="task"/>
</s:render>
</s:with>
<s:task b:action="select" b:target="id('layout-subtabbar-tasks')//s25:tabitem[@c:purpose = 'task_agenda']"/>
<s:task b:action="select" b:target="ancestor::div[@b:behavior='LayoutTabboxGroup' or @b:behavior='LayoutTabboxBuffer']"/>
<s:render b:destination="." b:mode="replacechildren">
<s25:task_agenda/>
</s:render>
</s:event>
<s:event b:on="PaintTaskDetails">
<s:with b:target="id('layout-subtabbar-tasks')">
<s:if b:test="not(./s25:subtabbar)">
<s:render b:destination="." b:mode="replacechildren">
<s25:subtabbar c:purpose="task"/>
</s:render>
</s:if>
</s:with>
<s:variable b:name="lTaskId" b:scope="local" b:select="id('Controller')/$tInbox/selected_obj/@obj_id"/>
<s:variable b:name="lTaskType" b:scope="local" b:select="id('Controller')/$tInbox/selected_obj/@task_type"/>
<s:render b:destination="." b:mode="replacechildren">
<s25:task_details c:purpose="{concat('task_details_',@c:obj_id)}" c:task_type="{$lTaskType}" c:obj_id="{@c:obj_id}"/>
</s:render>
<s:task b:action="trigger" b:event="UpdateIndividualDetailsTab" b:target="id('layout-subtabbar-tasks')/s25:subtabbar"/>
</s:event>
<s:event b:on="PaintPublisherOverview">
<s:with b:target="id('layout-subtabbar-publisher')">
<s:render b:test="not(./s25:subtabbar)" b:destination="." b:mode="replacechildren">
<s25:subtabbar c:purpose="publisher"/>
</s:render>
</s:with>
<s:task b:action="select" b:target="ancestor::div[@b:behavior='LayoutTabboxGroup' or @b:behavior='LayoutTabboxBuffer']"/>
<s:render b:destination="." b:mode="replacechildren">
<s25:publisher_admin/>
</s:render>
</s:event>
<s:event b:on="PaintRsrvWizard">
<s:with b:target="id('bxLoading')">
<s:task b:action="settext" b:target="$tMain" b:value="Loading Event Wizard..."/>
<s:task b:action="trigger" b:event="ShowLoading" b:target="."/>
</s:with>
<s:with b:target="id('layout-subtabbar-rsrvwiz')">
<s:render b:test="not(./s25:subtabbar)" b:destination="." b:mode="replacechildren">
<s25:subtabbar c:purpose="rsrvwiz"/>
</s:render>
</s:with>
<s:task b:action="select" b:target="ancestor::div[@b:behavior='LayoutTabboxGroup' or @b:behavior='LayoutTabboxBuffer']"/>
<s:render b:destination="." b:mode="replacechildren">
<s25:rsrv_wiz c:purpose="{$pPurpose}" c:contact_id="{id('WSSessionMgr')/@c:contact-id}"/>
</s:render>

</s:event>
</s:behavior>
<s:behavior b:name="TooltipToggle">
<s:event b:on="construct">
<s:variable b:name="lTooltipsDisabled" b:select="'false'"/>
<s:choose>
<s:when b:test="id('WSSessionMgr')/@c:logged-in='true' and string(id('Pref_25L_disable_tooltips')) = 'true'">
<s:task b:action="assign" b:target="$lTooltipsDisabled" b:select="'true'"/>
</s:when>
<s:when b:test="cookie('TooltipsDisabled') = 'true'">
<s:task b:action="assign" b:target="$lTooltipsDisabled" b:select="'true'"/>
</s:when>
</s:choose>
<s:choose>
<s:when b:test="$lTooltipsDisabled = 'true'">
<s:setatt c:status="disabled"/>
<s:task b:action="settext" b:value="Enable"/>
<s:task b:action="addclass" b:target="//body[1]" b:value="hideTooltips"/>
</s:when>
<s:otherwise>
<s:setatt c:status="enabled"/>
<s:task b:action="settext" b:value="Disable"/>
</s:otherwise>
</s:choose>
</s:event>
<s:event b:on="command">
<s:choose>
<s:when b:test="@c:status = 'disabled'">
<s:setatt c:status="enabled"/>
<s:task b:action="settext" b:value="Disable"/>
<s:task b:action="removeclass" b:target="//body[1]" b:value="hideTooltips"/>
<s:if b:test="id('WSSessionMgr')/@c:logged-in='true'">
<s:task b:action="settext" b:target="id('Pref_25L_disable_tooltips')" b:value="false"/>
<s:task b:action="trigger" b:event="Save" b:target="id('Pref_25L_disable_tooltips')"/>
</s:if>
<s:task b:action="setcookie" b:name="TooltipsDisabled" b:value="true" b:days="-1"/>
</s:when>
<s:otherwise>
<s:task b:action="settext" b:value="Enable"/>
<s:setatt c:status="disabled"/>
<s:task b:action="addclass" b:target="//body[1]" b:value="hideTooltips"/>
<s:if b:test="id('WSSessionMgr')/@c:logged-in='true'">
<s:task b:action="settext" b:target="id('Pref_25L_disable_tooltips')" b:value="true"/>
<s:task b:action="trigger" b:event="Save" b:target="id('Pref_25L_disable_tooltips')"/>
</s:if>
<s:task b:action="setcookie" b:name="TooltipsDisabled" b:value="true" b:days="365"/>
</s:otherwise>
</s:choose>
</s:event>
</s:behavior>
<s:behavior b:name="IncreaseFontSize">
<s:event b:on="construct">
<s:with b:target="id('fontSizeControl')">
<s:task b:test="number(@c:currentfontsize) = number(@c:largestfontsize)" b:target="id('fontSizeLarger')" b:action="addclass" b:value="disabledControl"/>
</s:with>
</s:event>
<s:event b:on="command">
<s:task b:action="blur"/>
<s:with b:target="id('fontSizeControl')">
<s:if b:test="number(@c:currentfontsize) lt number(@c:largestfontsize)">
<s:task b:action="removeclass" b:target="/body" b:value="{concat('font', string(@c:currentfontsize))}"/>
<s:task b:action="set" b:target="@c:currentfontsize" b:value="{number(@c:currentfontsize) + 1}"/>
<s:task b:action="addclass" b:target="/body" b:value="{concat('font', string(@c:currentfontsize))}"/>
<s:task b:test="number(@c:currentfontsize) = number(@c:largestfontsize)" b:target="id('fontSizeLarger')" b:action="addclass" b:value="disabledControl"/>
<s:task b:target="id('fontSizeSmaller')" b:action="removeclass" b:value="disabledControl"/>
<s:task b:action="setcookie" b:name="FontSize" b:value="{string(@c:currentfontsize)}" b:days="365"/>
</s:if>
</s:with>
</s:event>
</s:behavior>
<s:behavior b:name="DecreaseFontSize">
<s:event b:on="construct">
<s:with b:target="id('fontSizeControl')">
<s:task b:test="number(@c:currentfontsize) = number(@c:smallestfontsize)" b:target="id('fontSizeSmaller')" b:action="addclass" b:value="disabledControl"/>
</s:with>
</s:event>
<s:event b:on="command">
<s:task b:action="blur"/>
<s:with b:target="id('fontSizeControl')">
<s:if b:test="number(@c:currentfontsize) gt number(@c:smallestfontsize)">
<s:task b:action="removeclass" b:target="/body" b:value="{concat('font', string(@c:currentfontsize))}"/>
<s:task b:action="set" b:target="@c:currentfontsize" b:value="{number(@c:currentfontsize) - 1}"/>
<s:task b:action="addclass" b:target="/body" b:value="{concat('font', string(@c:currentfontsize))}"/>
<s:task b:test="number(@c:currentfontsize) = number(@c:smallestfontsize)" b:target="id('fontSizeSmaller')" b:action="addclass" b:value="disabledControl"/>
<s:task b:target="id('fontSizeLarger')" b:action="removeclass" b:value="disabledControl"/>
<s:task b:action="setcookie" b:name="FontSize" b:value="{string(@c:currentfontsize)}" b:days="365"/>
</s:if>
</s:with>
</s:event>
</s:behavior>
<s:behavior b:name="ResetFontSize">
<s:event b:on="command">
<s:task b:action="blur"/>
<s:with b:target="id('fontSizeControl')">
<s:task b:action="removeclass" b:target="/body" b:value="{concat('font', string(@c:currentfontsize))}"/>
<s:task b:action="set" b:target="@c:currentfontsize" b:value="{string(@c:defaultfontsize)}"/>
<s:task b:action="addclass" b:target="/body" b:value="{concat('font', string(@c:defaultfontsize))}"/>
<s:task b:target="id('fontSizeSmaller')" b:action="removeclass" b:value="disabledControl"/>
<s:task b:target="id('fontSizeLarger')" b:action="removeclass" b:value="disabledControl"/>
<s:task b:action="setcookie" b:name="FontSize" b:value="{string(@c:defaultfontsize)}" b:days="-1"/>
</s:with>
</s:event>
</s:behavior>
</div>