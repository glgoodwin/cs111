; SUM2: renames X in MULTIPLE? to N
(fobs (n lo hi)
  (funrec (sum-loop lo 0)
    (def (multiple? n) (= 0 (rem n)))
    (def (rem y) (% y n))
    (def (sum-loop i sum)
           (if (> i hi)
               sum
               (sum-loop (+ i 1)
                         (if (multiple? i) (+ i sum) sum))))))
