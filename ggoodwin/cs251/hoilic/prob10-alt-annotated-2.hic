(bindrec ((map (fun (f xs)
		 (if (empty? xs)
		     xs
		     (prep (f (head xs)) (map f (tail xs)))))))
  (bindpar ((a 1)
	    (b 1))
    (bindpar ((a 20)
	      (z a))
      (bind add! (abs x (seq (<- z (+ x (seq (print "calling add! where z=") ; Swapped X and Z in +
					     (println z))))
			     z))
	(bindrec ((s (prep b t))
		  (t (map add! s)))
	  (seq (println "")
	       (+ (seq (print "(head t)=")
		       (println (head t)))
		  (seq (print "(head (tail t))=")
		       (println (head (tail t)))))))))))
